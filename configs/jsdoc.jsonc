{
  // [JSDOC] https://github.com/gajus/eslint-plugin-jsdoc/tree/main/.README#rules
  "check-access": "error",
  "check-alignment": "error",
  "check-examples": "off", // not supported in eslint@8, only with weird config in eslint@9
  "check-indentation": "error",
  "check-line-alignment": "off",
  "check-param-names": "error",
  "check-property-names": "error",
  "check-syntax": "error",
  "check-tag-names": "error",
  "check-template-names": "error",
  "check-types": "error",
  "check-values": "error",
  "convert-to-jsdoc-comments":"off",
  "empty-tags": "error",
  "implements-on-classes": "error",
  "imports-as-dependencies": "error",
  "informative-docs": "error",
  "match-description": "off",
  "match-name": "off",
  "multiline-blocks": [
    "error",
    {
      "noFinalLineText": false
    }
  ],
  "no-bad-blocks": "error",
  "no-blank-block-descriptions": "error",
  "no-blank-blocks": [
    "error",
    {
      "enableFixer": true
    }
  ],
  "no-defaults": "error",
  "no-missing-syntax": "off",
  "no-multi-asterisks": [
    "warn",
    {
      "preventAtEnd": false
    }
  ],
  "no-restricted-syntax": "off",
  "no-types": "off",
  "no-undefined-types": [
    "error",
    {
      "markVariablesAsUsed": false,
      "definedTypes": [
        "NodeJS"
      ]
    }
  ],
  "require-asterisk-prefix": [
    "warn",
    "always"
  ],
  "require-description-complete-sentence": "off",
  "require-description": "off",
  "require-example": "off",
  "require-file-overview": "off",
  "require-hyphen-before-param-description": [
    "error",
    "never"
  ],
  "require-jsdoc": "off",
  "require-param-description": "off",
  "require-param-name": "error",
  "require-param-type": "error",
  "require-param": [
    "error",
    {
      "enableFixer": false,
      "useDefaultObjectProperties": true
    }
  ],
  "require-property-description": "off",
  "require-property-name": "error",
  "require-property-type": "error",
  "require-property": "error",
  "require-returns-check": [
    "error",
    {
      "exemptAsync": false
    }
  ],
  "require-returns-description": "off",
  "require-returns-type": "error",
  "require-returns": "off",
  "require-template": "off",
  "require-throws": "error",
  "require-yields-check": "error",
  "require-yields": "off",
  "sort-tags": [
    "warn",
    {
      "linesBetween": 0,
      "tagSequence": [
        {
          "tags": [
            // Copied from https://github.com/gajus/eslint-plugin-blob/main/docs/rules/sort-tags.md#tagsequence
            // Brief descriptions
            "summary",
            "typeSummary",
            // Module/file-level
            "module",
            "exports",
            "file",
            "fileoverview",
            "overview",
            // Identifying (name, type)
            "typedef",
            "interface",
            "record",
            "template",
            "name",
            "kind",
            "type",
            "alias",
            "external",
            "host",
            "callback",
            "func",
            "function",
            "method",
            "class",
            "constructor",
            // Relationships
            "modifies",
            "mixes",
            "mixin",
            "mixinClass",
            "mixinFunction",
            "namespace",
            "borrows",
            "constructs",
            "lends",
            "implements",
            "requires",
            // Long descriptions
            "desc",
            "description",
            "classdesc",
            "tutorial",
            "copyright",
            "license",
            // Simple annotations
            "const",
            "constant",
            "final",
            "global",
            "readonly",
            "abstract",
            "virtual",
            "var",
            "member",
            "memberof",
            "memberof!",
            "inner",
            "instance",
            "inheritdoc",
            "inheritDoc",
            "override",
            "hideconstructor",
            // Core function/object info
            "this",
            "param",
            "arg",
            "argument",
            "prop",
            "property",
            "return",
            "returns",
            // Important behavior details
            "async",
            "generator",
            "default",
            "defaultvalue",
            "enum",
            "augments",
            "extends",
            "throws",
            "exception",
            "yield",
            "yields",
            "event",
            "fires",
            "emits",
            "listens",
            // Access
            "static",
            "private",
            "protected",
            "public",
            "access",
            "package",
            "-other",
            // Supplementary descriptions
            "see",
            "example",
            // METADATA
            // Other Closure (undocumented) metadata
            "closurePrimitive",
            "customElement",
            "expose",
            "hidden",
            "idGenerator",
            "meaning",
            "ngInject",
            "owner",
            "wizaction",
            // Other Closure (documented) metadata
            "define",
            "dict",
            "export",
            "externs",
            "implicitCast",
            "noalias",
            "nocollapse",
            "nocompile",
            "noinline",
            "nosideeffects",
            "polymer",
            "polymerBehavior",
            "preserve",
            "struct",
            "suppress",
            "unrestricted",
            // @homer0/prettier-plugin-jsdoc metadata
            "category",
            // Non-Closure metadata
            "ignore",
            "author",
            "version",
            "variation",
            "since",
            "deprecated",
            "todo"
          ]
        }
      ]
    }
  ],
  "tag-lines": "off", //Handled by other jsdoc rules
  "text-escaping": "off",
  "valid-types": "error"
}